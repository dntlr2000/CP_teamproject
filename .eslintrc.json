{
    "extends": "airbnb",
    "plugins": [
      "react",
      "react-hooks"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "no-use-before-define": "off",
      "import/prefer-default-export": "off"
    }
  }
  